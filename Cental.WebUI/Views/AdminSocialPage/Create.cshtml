@model CreateSocialPageDto
@{
    ViewData["Title"] = "Cental Sosyal Media Sayfa Ekleme";
    Layout = "~/Views/AdminLayout/Layout.cshtml";
}


<form method="post">
    <label class="form-label">Sayfa Adı</label>
    <input type="text" asp-for="@Model.Name" class="form-control" /> <br />
    <label class="form-label">Sayfa ikonu</label>
    <input type="text" asp-for="@Model.Icon" class="form-control" /> <br />
    <label class="form-label"> Sayfa Url Adresi</label>
    <input class="form-control" type="text" asp-for="@Model.Url" /> <br />
    <button class="btn btn-primary" type="submit"><i class="fa fa-plus-circle me-2"></i>Kaydet</button>
</form>


